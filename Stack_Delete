std::vector<int> removeElements(std::vector<int> arr, int k)
{
    int m = 0;
    stack<int> st;
    vector<int> v;
    st.push(arr[0]);
    v.push_back(arr[0]);
    for(int i = 1; i < arr.size(); i++)
    {
        while(!st.empty() && arr[i] > st.top() && m < k)
        {
            st.pop();
            v.pop_back();
            m++;
        }
        v.push_back(arr[i]);
        st.push(arr[i]);
    }
    return v;
}
